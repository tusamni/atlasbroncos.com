{
	"extends": "astro/tsconfigs/base",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/assets/*": [
				"src/assets/*"
			],
			"@/components/*": [
				"src/components/*.astro"
			],
			"@/config": [
				"src/config.ts"
			],
			"@/functions/*": [
				"src/library/functions/*.js"
			],
			"@/layouts/*": [
				"src/layouts/*.astro"
			],
			"@/library/*": [
				"src/library/*.ts"
			],
			"@/public/*": [
				"public/*"
			],
			"@/src/*": [
				"src/*"
			],
		}
	}
}